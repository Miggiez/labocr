{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"shell:allow-open",
		{
			"identifier": "shell:allow-execute",
			"allow": [
				{
					"name": "bin/python/stream",
					"sidecar": true
				},
				{
					"name": "bin/python/camera",
					"sidecar": true
				}
			]
		}
	]
}
